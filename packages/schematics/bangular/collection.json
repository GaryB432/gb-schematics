{
  "schematics": {
    "workspace": {
      "factory": "./workspace",
      "schema": "./workspace/schema.json",
      "description": "Create an Angular workspace.",
      "hidden": true
    },
    "component": {
      "aliases": ["c"],
      "factory": "./component",
      "description": "Create an Angular component.",
      "schema": "./component/schema.json"
