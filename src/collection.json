{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "eslint": {
      "description": "Add eslint configuration.",
      "schema": "./eslint/schema.json",
      "factory": "./eslint/index#eslint"
    },
    "prettier": {
      "description": "Add prettier configuration.",
      "factory": "./prettier/index#prettier"
    },
    "typescript": {
      "description": "Add typescript.",
      "factory": "./typescript/index#typescript"
    },
    "devops": {
      "description": "Add platform devops pipeline",
      "schema": "./devops/schema.json",
      "factory": "./devops/index#devops"
    },
    "sveltekit-component": {
      "description": "Add a component to your sveltekit project",
      "schema": "./sveltekit-component/schema.json",
      "factory": "./sveltekit-component"
    },
    "sveltekit-route": {
      "description": "Add a route to your sveltekit project",
      "schema": "./sveltekit-route/schema.json",
      "factory": "./sveltekit-route"
    },
    "jest": {
      "description": "Add Jest testing",
      "factory": "./jest/index#jestFactory"
    },
    "bump": {
      "description": "Bump package.json version",
      "schema": "./bump/schema.json",
      "factory": "./bump/index#bump"
    }
  }
}
