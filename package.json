{
  "name": "gb-schematics",
  "version": "1.0.0",
  "description": "Schematics I like",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" --single-quote --trailing-comma es5",
    "lint": "tslint -c tslint.json \"src/**/*.ts\"",
    "prebuild": "npm run format",
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine **/*_spec.js"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "ISC",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^0.6.8",
    "@angular-devkit/schematics": "^0.6.8",
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.3.5",
    "jasmine": "^3.1.0",
    "prettier": "^1.13.5",
    "tslint": "^5.10.0",
    "tslint-config-gb": "^1.1.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.9.2"
  }
}
