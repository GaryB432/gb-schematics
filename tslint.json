{
    "extends": [
        "tslint:latest",
        "tslint-config-prettier",
        "tslint-config-gb"
    ],
    "jsRules": {},
    "rules": {
        "no-submodule-imports": [
            true,
            "rxjs",
            "@angular/platform-browser",
            "@angular/core/testing"
        ]
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "src/*/files/**/*",
            "src/*/other-files/**/*"
        ]
    }
}